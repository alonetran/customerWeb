# Default ignored files
/shelf/
/workspace.xml
# Datasource local storage ignored files
/dataSources/
/dataSources.local.xml
# Editor-based HTTP Client requests
/httpRequests/


# .gitignore for static HTML/CSS website

# Ignore files and directories generated by development tools
/node_modules
/.vscode
/.idea

# Ignore build output
/build
/dist

# Ignore package-lock.json for npm projects or yarn.lock for Yarn projects
/package-lock.json
/yarn.lock

# Ignore any log files
*.log

# Ignore temporary files
*.tmp

# Ignore files and directories containing sensitive information
/config.js
/credentials.json

# Ignore hidden system files and directories (e.g., .DS_Store on macOS)
.DS_Store

# Ignore cache and generated files
*.cache
*.html~
*.css.map

# Ignore backup files
*.bak
*.backup

# Ignore any other files you might generate or use during development
